Pair Programming
Grand Circus
11/20/17

CREATE TABLE employee (
SSN INT (9) UNSIGNED NOT NULL PRIMARY KEY,
SALARY DOUBLE UNSIGNED NOT NULL, 
PHONE INT (10) UNSIGNED NOT NULL);

CREATE TABLE dept (
dno INT UNSIGNED NOT NULL PRIMARY KEY,
dname varchar(20) NOT NULL,
budget DOUBLE UNSIGNED NOT NULL);

CREATE TABLE children(
AGE INT (2) UNSIGNED NOT NULL,
Child_Name  VARCHAR (30) NOT NULL,
EMPLOYEE_SSN INT (9) UNSIGNED NOT NULL,
FOREIGN KEY (EMPLOYEE_SSN) REFERENCES employee(SSN),
PRIMARY KEY(Employee_SSN, Child_Name));
